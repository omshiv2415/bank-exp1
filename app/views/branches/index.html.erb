<p id="notice"><%= notice %></p>

<h1>Listing Branches</h1>

<table>
  <thead>
    <tr>
      <th>Branchlocation</th>
      <th>Branchcity</th>
      <th>Branchcounty</th>
      <th>Branchcode</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @branches.each do |branch| %>
      <tr>
        <td><%= branch.branchLocation %></td>
        <td><%= branch.branchCity %></td>
        <td><%= branch.branchCounty %></td>
        <td><%= branch.branchCode %></td>
        <td><%= link_to 'Show', branch %></td>
        <td><%= link_to 'Edit', edit_branch_path(branch) %></td>
        <td><%= link_to 'Destroy', branch, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Branch', new_branch_path %>
